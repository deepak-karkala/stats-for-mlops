{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/data/(.*).csv",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/chapters/chapter-(.*)/fixtures/(.*).csv",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).csv",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/chapters/chapter-1"
    }
  ]
}
